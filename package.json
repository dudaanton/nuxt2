{
  "name": "nuxt_build",
  "version": "1.0.0",
  "description": "nuxt_build",
  "author": "AntonD",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=3001 nodemon server/index.js --watch server",
    "build": "nuxt build -c config/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate -c config/index.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "icons": "node ./bin/icons.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clear": "^0.1.0",
    "cross-env": "^5.2.0",
    "koa": "^2.5.2",
    "nuxt": "^2.0.0",
    "sanitize.css": "^8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^8.2.1",
    "chalk": "^2.4.1",
    "cross-spawn": "^6.0.5",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "i": "^0.3.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.11.0",
    "ora": "^3.0.0",
    "prettier": "1.14.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "vue-svgicon": "^3.2.2"
  }
}
